
project samoa ;

lib libsamoa :
    [ glob-tree *.cpp ] $(BUILD)/generated/cpp_src/samoa/core/protobuf/samoa.pb.cc
    /root//boost_system
    /root//boost_thread
    /root//boost_regex
    /root//boost_random
    /root//protobuf
    : <link>shared
    ;

install dist : libsamoa : <location>$(INSTALL) ;

